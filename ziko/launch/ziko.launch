<?xml version="1.0"?>
<launch>
  <arg name="model" default="ziko"/>
  <arg name="x_pos" default="3.0"/>
  <arg name="y_pos" default="0.0"/>
  <arg name="z_pos" default="5.0"/>
  <arg name="verbose" default="true"/>

  <remap from="/gps/fix" to="/fix"/>
  
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
	  <arg name="world_name" value="$(find ziko)/worlds/empty.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model"
	    args="-z 1.0 -unpause -sdf -model ziko -file $(find ziko)/model/model.sdf  " respawn="false" output="screen" />

  <node 
    pkg="robot_localization" 
    type="ekf_localization_node" 
    name="ekf" 
    clear_params="true">
    <rosparam command="load" file="$(find ziko)/param/ekf_params.yaml"/>
   </node>

  <include file="$(find husky_navigation)/launch/move_base.launch">
    <arg name="no_static_map" value="true"/>
    <!--arg name="max_vel_trans" value="0.2"/>
    <arg name="max_vel_theta" value="0.1"/-->

  </include>
  <!--[ WARN] [1559864594.879642214, 0.393000000]: Parameter max_trans_vel is deprecated (and will not load properly). Use max_vel_trans instead.
[ WARN] [1559864594.880236875, 0.394000000]: Parameter min_trans_vel is deprecated (and will not load properly). Use min_vel_trans instead.
[ WARN] [1559864594.880765424, 0.394000000]: Parameter max_rot_vel is deprecated (and will not load properly). Use max_vel_theta instead.
[ WARN] [1559864594.881341211, 0.395000000]: Parameter min_rot_vel is deprecated (and will not load properly). Use min_vel_theta instead.
[ INFO] [1559864595.105549635, 0.619000000]: Recovery behavior will clear layer obstacles
-->
   <!--node
     pkg="robot_localization" 
     type="navsat_transform_node" 
     name="navsat_transform" >
    <rosparam command="load" file="$(find ziko)/param/navsat_trans.yaml"/>
   </node-->

</launch>

